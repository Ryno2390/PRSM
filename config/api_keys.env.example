# PRSM API Configuration
# Copy this file to api_keys.env and fill in your API keys

# OpenAI API Key
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key  
# Get from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Hugging Face API Key
# Get from: https://huggingface.co/settings/tokens
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Local Models Path
# Directory containing PRSM distilled models
PRSM_LOCAL_MODELS_PATH=/path/to/your/local/models

# Rate Limiting Configuration
OPENAI_RATE_LIMIT_RPM=3500
ANTHROPIC_RATE_LIMIT_RPM=1000
HUGGINGFACE_RATE_LIMIT_RPM=1000

# Fallback Configuration
ENABLE_MODEL_FALLBACKS=true
DEFAULT_FALLBACK_MODEL=gpt-3.5-turbo

# Cost Tracking
TRACK_API_COSTS=true
FTNS_COST_MULTIPLIER=0.1